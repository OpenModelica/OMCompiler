outputFormat:=\"default\";
javascript:="+String(javascript)+";
mslRegressionOutput:=\"\";
"+loadModelCommand+"
reference_reltol:="+String(reference_reltol)+";
reference_reltolDiffMinMax:="+String(reference_reltolDiffMinMax)+";
